<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <%- include('../partial/navigator.ejs') %>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>D2C</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <script src="../csv/csv.js"></script>
</head>
<body style="background-color:rgb(235, 230, 226)">
    <div class="container">
        <h1 class="header">Add new CSV</h1>
            <div class="row">
                <div class="input-field">
                    <% try {%>
                    <form action="/fillTests/xlsx" method="POST" enctype="multipart/form-data" target="hiddenFrame">
                        <br></br>
                        <input v-model="csv_upload" ref="csv_upload" placeholder="Input URL" name="csvUpload" type="file" class="validate"  accept=".csv,.xlsx">
                        <input v-model="id" ref="id" placeholder="Enter ID" name="hash" type="text" class="validate">
                        <input placeholder="Enter Age" name="age" type="text" class="validate">
                        <input placeholder="Enter Education" name="education" type="text" class="validate">
                        <label>
                            <input type='radio' name='gender' value='Male'>
                            <span>Male</span>
                        </label>
                        <label>
                            <input type='radio' name='gender' value='Female'>
                            <span>Female</span>
                        </label>
                    </br>
                        <button id='send' class='btn-small' style="background-color: rgb(78, 90, 81);" ><i class='material-icons right'>send</i>send</button>
                        <br></br>
                        <input v-model="id" ref="id" placeholder="Enter ID" name="hash" type="text" class="form-control" required>
                        <br></br>
                        <button id='send' class='btn-small' style="background-color: rgb(78, 90, 81);"><i class='material-icons right'>add</i>send</button>
                    </form>
                    <% } catch(err) { } %>
                    <br></br>
                    <iframe name="hiddenFrame" id="hiddenFrame" hidden="true"></iframe>
                </div>
            </div>
    </div>
</body>
</html>